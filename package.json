{
  "name": "kibo-jobboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/storybook-addon": "^4.0.16",
    "@directus/sdk": "10.3.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/manrope": "^4.5.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/airtable": "^0.10.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/sanitize-html": "^2.9.0",
    "airtable": "^0.11.6",
    "axios": "^1.3.4",
    "framer-motion": "^6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "sanitize-html": "^2.11.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "deploy": "yarn ask && react-scripts build && gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "migrate-collection": "ts-node-dev ./src/scripts/migrate-collection.ts",
    "airtable-migrate-files": "ts-node-dev ./src/scripts/airtable-migrate-files.ts",
    "ask": "node -e \"const readlineSync = require('readline-sync'); console.log('Check your .env file before continuing with deploy (press enter when ready)'); const input = readlineSync.question(); console.log(input);\"",
    "import-images": "ts-node-dev ./src/scripts/import-images.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "csv-parser": "^3.0.0",
    "gh-pages": "^5.0.0",
    "prop-types": "^15.8.1",
    "readline-sync": "^1.4.10",
    "ts-node-dev": "^2.0.0",
    "webpack": "^5.75.0"
  }
}
